package edu.umass.cs.pig.dataflow;

import java.io.Serializable;

public class ParentValueTNype implements Serializable {
	/**
	 * 
	 */
	private static final long serialVersionUID = 1L;
	Object val;
	byte type;
	public Object getVal() {
		return val;
	}
	public void setVal(Object val) {
		this.val = val;
	}
	public byte getType() {
		return type;
	}
	public void setType(byte type) {
		this.type = type;
	}
	public ParentValueTNype(Object val, byte type) {
		super();
		this.val = val;
		this.type = type;
	}
	
//	public ParentValueTNype(Object val) {
//		super();
//		this.val = val;
//	}

}
